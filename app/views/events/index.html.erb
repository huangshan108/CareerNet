<h1>All Events</h1>
<% @events.each do |event| %>
  <tr>
    <% if !event.title.nil? %>
      <td><%= link_to event.title, event_path(event) %></td>
      <td><%= event.time_start %></td>
      <td><%= event.time_end %></td><br>
    <% end %>
  </tr>
<% end %>
<br>
<div class="add-event">
  <%= link_to 'Add an event', new_event_path %>
</div>

