Welcome to CareerNet

<table style="width:40%">
  <tr>
  	<th> Job </th>
  	<th> Company </th>
  	<th> Description </th>
  	<th> Salary </th>
  	<th> Created </th>
  	<th> Added </th>
  </tr>
  <% @jobs.each do |job| %>
	  <tr>
	    <td><%= job.title %></td>
	    <td><%= job.company %></td>
	    <td><%= job.description %></td>
	    <td><%= job.salary %></td>
	    <td><%= job.created_at %></td>
	    <td><%= job.updated_at %></td>
	  </tr>
	  <% end %>
</table>


   t.string   "title"
    t.text     "description"
    t.string   "company"
    t.string   "salary"
    t.datetime "created_at"
    t.datetime "updated_at"