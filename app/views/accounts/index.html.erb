<div class="video-overlay">
	
</div>
<video autoplay loop id="home_page_video" poster="https://dl.dropbox.com/s/7d9ghc3tp80s37a/home_page.jpg?dl=0">
	<source src="https://dl.dropbox.com/s/cfswa69v0z0fg6i/home_page.mp4?dl=0" type="video/mp4">
</video>
<div class="welcome">
	<div class="title">
		CareerNet
	</div>
	<div class="description">
		Your Brand New Future Starts Here
	</div>
	<div class="explore">
		<%= link_to "Explore My Future", dashboard_path %>
	</div>
</div>
<script type="text/javascript">
	$(function() {
		$('.welcome .title').delay(300).addClass('animated fadeInDown');
		$('.welcome .description').delay(300).addClass('animated fadeInDown');
		$('.welcome .explore').delay(300).addClass('animated fadeInUp');
	})
</script>
